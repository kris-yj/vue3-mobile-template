{
  "name": "vue3-mobile-template",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:format",
    "lint:js": "eslint src --fix --ext .js,.jsx,.ts,.tsx,.vue --cache --cache-location node_modules/.cache/eslint/",
    "lint:css": "stylelint  \"src/**/*.{less,postcss,css,scss,vue}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:format": "prettier --write  \"src/**/*.{js,json,ts,tsx,vue,css,less,scss,html,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kris-yj/vue3-mobile-template.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kris-yj/vue3-mobile-template/issues"
  },
  "homepage": "https://github.com/kris-yj/vue3-mobile-template#readme",
  "lint-staged": {
    "src/**/*.{vue,js,jsx,ts,tsx,css,scss,less,postcss,json}": "npm run lint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "axios": "^0.26.1",
    "crypto-js": "^4.1.1",
    "js-md5": "^0.7.3",
    "vant": "^3.4.5",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-service": "^5.0.1",
    "browserslist": "^4.20.0",
    "camelcase": "^6.3.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filename": "^1.0.3",
    "eslint-plugin-sonarjs": "^0.11.0",
    "eslint-plugin-vue": "^8.4.1",
    "husky": "^4.3.7",
    "lint-staged": "^12.3.3",
    "postcss-html": "^1.3.0",
    "postcss-nested": "^5.0.6",
    "prettier": "2.5.1",
    "stylelint": "^14.4.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.5.0",
    "stylelint-no-unsupported-browser-features": "^5.0.2",
    "stylelint-order": "^5.0.0",
    "vue-eslint-parser": "^8.2.0"
  }
}
